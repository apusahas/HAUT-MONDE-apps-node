<html>
    <head>
        <link rel="stylesheet" href="../public/stylesheets/style.css" />
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Inter" />
    </head>
    
    <body class="height-100vh">
        <div class="logged_in_nav">
            <div class="flex">
                <div>
                    <img src="https://cdn-icons-png.flaticon.com/512/1903/1903162.png" alt="workos logo" />
                </div>
            </div>
            <div class="flex">
                <a href="">
                    <button class="button nav-item">About Us</button>
                </a>
                <a href="">
                    <button class="button nav-item">Meetups</button>
                </a>
                <a href="">
                    <button class="button nav-item blog-nav-button">Books</button>
                </a>
                <a href="">
                    <button class="button button-outline">Apply Now</button>
                </a>
            </div>
        </div>
        <div class="flex">
            <div class="logged_in_div_right">
                <div class="flex_column">
                    <h2>Directory Details</h2>
                    <div>
                        <pre id="noborder" class="prettyprint noborder">
                            <%- JSON.stringify(directory, null, 2) %>
                        </pre>
                    </div>
                </div>
                <div class="flex width-440px space-between">
                    <div class="flex">
                        <a href="/users?id=<%=directory.id%>">
                            <button class="button">Users</button>
                        </a>
                        <br>
                        <a href="/groups?id=<%=directory.id%>">
                            <button class="button">Groups</button>
                        </a>
                    </div>
                    <div>
                        <a style="align-self:flex-end; padding:10px;" href="javascript:history.back()">
                            <button class="button button-sm button-outline">Back</button>
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <script src="https://cdn.jsdelivr.net/gh/google/code-prettify@master/loader/run_prettify.js"></script>
    </body>
</html>