<html>
    <head>
        <link rel="stylesheet" href="../public/stylesheets/style.css" />
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Inter" />
    </head>
    
    <body class="height-100vh">
        <div class="logged_in_nav">
            <div class="flex">
                <div>
                    <img src="https://cdn-icons-png.flaticon.com/512/1903/1903162.png" alt="workos logo" />
                </div>
            </div>
            <div class="flex">
                <a href="">
                    <button class="button nav-item">About Us</button>
                </a>
                <a href="">
                    <button class="button nav-item">Meetups</button>
                </a>
                <a href="">
                    <button class="button nav-item blog-nav-button">Books</button>
                </a>
                <a href="">
                    <button class="button button-outline">Apply Now</button>
                </a>
            </div>
        </div>
        <div class='flex'>
            <div class="logged_in_div_right">
                <div class="flex_column">
                    <div class="flex space-between width-507px">
                        <div>
                            <h2>User Details</h2>
                        </div>
                        <div>
                            <a href="javascript:history.back()">
                                <button class='button button-sm button-outline'>Back</button>
                            </a>
                        </div>
                    </div>
                    <% if (users.length > 0) { %>
                    <div class="table-container">
                        <table class="width-507px">
                            <tr>
                                <th>Name</th>
                                <th>Email</th>
                            </tr>
                            <% for (let i = 0; i < users.length; i++) { %>
                            <tr>
                                <td class="ta-left"><%= users[i].first_name %> <%= users[i].last_name %></td>
                                <td class="ta-left"><code><%= users[i].username %></code></td>
                            </tr>
                            <% } %>
                        </table>
                    </div>
                    <% } else { %>
                    <div class="card width-40vw">
                        <code>No Users Found</code>
                    </div>
                    <% } %>
                </div>
            </div>
        </div>
    </body>
</html>
